{
    "version": 2,
    "builds": [
        {
            "src": "backend/main_api/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/main_api/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "backend.main_api.settings"
    }
}