{
    "builds": [
        {
            "src": "main_api/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "main_api/wsgi.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "true",
        "DJANGO_SETTINGS_MODULE": "main_api.settings"
    }
}